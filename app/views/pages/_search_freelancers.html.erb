<div class="search__wrapper">
  <%= search_form_for q, url: search_results_path ,id:'search' do |f|%>
    <%= render 'search-filters-freelancers',f: f %>
    <div class="search_bar">
      <%= f.label :name_or_email_or_freelancer_info_category_cont,style:'display:none' %>
      <%= f.search_field :name_or_email_or_freelancer_info_category_cont, placeholder: "Search" %>
    </div>
    <%= button_tag class: ' search__btn'   do%>
      <i class="far fa-search"></i>
    <%end%>
    <%= link_to  request.path, class: "reset__btn" do%>
      <i class="fal fa-trash-alt"></i>
    <% end %>
  <% end %>
</div>

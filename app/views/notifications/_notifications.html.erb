<ul class="notifications__modal">
  <% unless notifications.nil? || notifications.empty? %>
    <%notifications.each do|notification|%>
      <%if  notification.user.role=="client"%>
        <%=render partial: 'clientNotification',locals:{notification: notification} %>
      <% else %>
        <%=render partial: 'freelancerNotification',locals:{notification: notification} %>
      <%end%>
    <%end%>
  <%else%>
    <div class="no__notifications">
      <%=inline_svg_tag 'notifications.svg'%>
      You have no notifications
    </div>
  <% end %>
</ul>

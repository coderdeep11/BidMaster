set_field = (element) => {
  element.value = "";
  element.placeholder = "search freelancers";
  document.querySelector(".search__dropdown").remove();
};

if (document.querySelector("#q_description_cont"))
  set_field(document.querySelector("#q_description_cont"));
if (document.querySelector("#q_name_cont"))
  set_field(document.querySelector("#q_name_cont"));
